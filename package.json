    "@types/nodemailer": "^6.4.16",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.4",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.16",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typeorm": "^0.3.20"
  }
